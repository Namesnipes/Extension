{

  "description": "Lovely journal and mood tracker extension",
  "manifest_version": 2,
  "name": "Moodify",
  "version": "0.1",
  "permissions": [
  "storage"
],
  "homepage_url": "https://github.com/Namesnipes/Extension",
  "icons": {
    "48": "assets/icon.jpg"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "main.html",
    "default_title": "Moodify"
  }

}
